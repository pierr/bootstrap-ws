<div class="">
  <div class="container">
    <div class="page-header">
      <h1>{{intro.title}}  <small>{{intro.subTitle}}</small></h1>
    </div>
    
    <p>
      {{intro.message}}
      <ul class="media-list">{{#each intro.tasks}}<li class="media"><div class="media-body">{{this}}</div><span class="glyphicon glyphicon-star badge">&nbsp</span></li>{{/each}}</ul>
    </p>
    <p><a href="mailto:{{contact.email}}?subject={{contact.subject}}&amp;body={{contact.body}}" class="btn btn-default btn-lg" role="button">{{intro.button}}</a></p>
  </div>
</div>